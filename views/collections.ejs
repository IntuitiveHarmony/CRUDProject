<!DOCTYPE html>
<html lang="en">
<head>
    <%-include('./partials/head.ejs')%>
</head>
<body>
  <header>
    <%-include('./partials/nav.ejs')%>

  </header>



  <nav class ='collectionNav'>
    <ul class='collectionLink shirtsBtn link'>Shirts</ul>
    <ul class='collectionLink pantsBtn link'>Pants</ul>
    <ul class='collectionLink skirtsBtn link'>Skirts/ Dresses</ul>
    <ul class='collectionLink hoodiesBtn link'>Hoodies</ul>
    <ul class='collectionLink kidsBtn link'>Kids</ul>
    <ul class='collectionLink miscBtn link'>Misc</ul>
  </nav>
    <div class ='cardContainer'>
      <%for (let i = 0; i < item.length; i++){%>
        <a href ='/show/<%= item[i]._id %>'>
          <div class ='card link <%= item[i].catagory %> '>
          <h3><%= item[i].name %></h3>
          <img src='<%= item[i].img %>' class = 'cardImg' alt ='<%= item[i].name %>' >
          </div>
        </a>
      <%}%>
    </div>


  </body>
  </html>
